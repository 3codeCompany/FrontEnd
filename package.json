{
    "name": "frontend",
    "version": "1.0.0",
    "description": "",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "scripts": {
        "dev": "webpack-dev-server -d ",
        "build:babel": "babel --extensions .ts,.tsx --source-maps inline -d ./lib ./src",
        "babel-watch": "babel --watch --extensions .ts,.tsx --source-maps inline -d ./lib ./src & tsc --watch --emitDeclarationOnly --project ./ --outDir ./lib",
        "build:tsc": "tsc --emitDeclarationOnly --project ./ --outDir ./lib ",
        "rebuild": "rimraf lib/* && yarn run build:babel  && yarn run build:tsc",
        "postinstall": "rimraf lib/* && yarn run build:babel ",
        "prepare": "yarn run build:tsc",
        "storybook": "start-storybook -c ./docs/stories/.storybook -s ./stories/public_assets -p 6006",
        "server": "nodemon ./stories/server.js",
        "website:build-all": "rimraf ./website-build && yarn run website:build-storybook && yarn run website:build-wiki && yarn run website:build-api-doc && yarn run website:build-page",
        "website:build-storybook": "build-storybook -c ./docs/stories/.storybook -o ./website-build/storybook",
        "website:build-api-doc": "yarn run typedoc --out website-build/api src",
        "website:build-page": "webpack-cli --config docs/website/webpack-server.config.js  && webpack-cli --env.production  --config docs/website/webpack.config.js",
        "website:build-wiki": "node --experimental-modules docs/wiki/build.js",
        "website:server-start": "node ./website-build/dist/server",
        "website:dev-page": "webpack-dev-server --https --hot  --port 3001  --config docs/website/webpack.config.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn run rebuild"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "prettier": {
        "useTabs": false,
        "tabWidth": 4,
        "printWidth": 120,
        "arrowParens": "always",
        "trailingComma": "all"
    },
