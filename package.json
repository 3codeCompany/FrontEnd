{
    "name": "frontend",
    "version": "1.0.0",
    "description": "",
    "main": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "scripts": {
        "dev": "webpack-dev-server -d ",
        "build:babel": "babel --extensions .ts,.tsx --source-maps inline -d ./lib ./src",
        "babel-watch": "babel --watch --extensions .ts,.tsx --source-maps inline -d ./lib ./src & tsc --watch --emitDeclarationOnly --project ./ --outDir ./lib",
        "build:tsc": "tsc --emitDeclarationOnly --project ./ --outDir ./lib ",
        "rebuild": "rimraf lib/* && npm run build:babel  && npm run build:tsc",
        "storybook": "start-storybook -c ./docs/stories/.storybook -s ./stories/public_assets -p 6006",
        "server": "nodemon ./stories/server.js",
        "website:build-all": "rimraf ./website-build && npm run website:build-storybook && npm run website:build-wiki && npm run website:build-api-doc && npm run website:build-page",
        "website:build-storybook": "build-storybook -c ./docs/stories/.storybook -o ./website-build/storybook",
        "website:build-api-doc": "npm run typedoc --out website-build/api src",
        "website:build-page": "webpack-cli --config docs/website/webpack-server.config.js  && webpack-cli --env.production  --config docs/website/webpack.config.js",
        "website:build-wiki": "node --experimental-modules docs/wiki/build.js",
        "website:server-start": "node ./website-build/dist/server",
        "website:dev-page": "webpack-dev-server --https --hot  --port 3001  --config docs/website/webpack.config.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run rebuild"
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "prettier": {
        "useTabs": false,
        "tabWidth": 4,
        "printWidth": 120,
        "arrowParens": "always",
        "trailingComma": "all"
    },
    "files": [
        "lib"
    ],
    "dependencies": {
        "@babel/core": "^7.8.6",
        "@babel/preset-env": "^7.8.6",
        "@babel/preset-react": "^7.9.4",
        "bootstrap-sass": "^3.4.1",
        "core-js": "3.6.5",
        "fork-awesome": "^1.2.0",
        "i18next": "^19.3.2",
        "inputmask": "^5.0.3",
        "keyboardjs": "^2.4.1",
        "moment": "^2.19.4",
        "nanoid": "^3.1.10",
        "nprogress": "^0.2.0",
        "office-ui-fabric-core": "7.1.0",
        "react": "^16.13.0",
        "react-addons-shallow-compare": "^15.6.2",
        "react-day-picker": "^7.4.0",
        "react-dom": "^16.13.0",
        "react-dropzone": "^8.0.3",
        "react-i18next": "^11.3.3",
        "react-notification-system": "^0.3.0",
        "react-sortable-hoc": "^1.11.0",
        "resize-observer-polyfill": "^1.5.1",
        "ts-key-enum": "2.0.2"
    },
    "devDependencies": {
        "@types/i18next": "^13.0.0",
        "@types/moment": "^2.13.0",
        "@types/nanoid": "^2.1.0",
        "@types/react-dates": "^17.1.10",
        "@types/react-dom": "^18.2.18",
        "@types/react-i18next": "^8.1.0",
        "@types/react-notification-system": "^0.2.39",
        "@types/react-virtualized-auto-sizer": "^1.0.0",
        "@types/uuid": "^8.0.0"
    },
    "resolutions": {}
}
